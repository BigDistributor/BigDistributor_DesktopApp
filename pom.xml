<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.scijava</groupId>
		<artifactId>pom-scijava</artifactId>
		<version>29.2.1</version>
		<relativePath />
	</parent>

	<groupId>com.bigdistributor</groupId>
	<artifactId>BD_Task</artifactId>
	<version>1.0-SNAPSHOT</version>

	<name>Maven Task AWS</name>
	<description>Needed library for task, just import and impliment</description>
	<url>https://github.com/BigDistributor</url>
	<inceptionYear>2020</inceptionYear>


	<organization>
		<name>Preibisch Lab</name>
		<url>https://preibischlab.mdc-berlin.de/</url>
	</organization>
	<licenses>
		<license>
			<name>Simplified BSD License</name>
			<distribution>repo</distribution>
		</license>
	</licenses>

	<developers>
		<developer>
			<id>mzouink</id>
			<name>Marwan Zouinkhi</name>
			<url>https://imagej.net/User:mzouink</url>
			<roles>
				<role>founder</role>
				<role>lead</role>
				<role>developer</role>
				<role>debugger</role>
				<role>reviewer</role>
				<role>support</role>
				<role>maintainer</role>
			</roles>
		</developer>
	</developers>
	<contributors>
		<contributor>
			<name>None</name>
		</contributor>
	</contributors>

	<mailingLists>
		<mailingList>
			<name>Image.sc Forum</name>
			<archive>None</archive>
		</mailingList>
	</mailingLists>

	<scm>
		<connection>scm:git:git://github.com/BigDistributor/BD_Distributor</connection>
		<developerConnection>scm:git:git@github.com:BigDistributor/BD_Distributor</developerConnection>
		<tag>HEAD</tag>
		<url>https://github.com/BigDistributor/BD_Distributor</url>
	</scm>
	<issueManagement>
		<system>GitHub</system>
		<url>https://github.com/BigDistributor/BD_Distributor/issues</url>
	</issueManagement>
	<ciManagement>
		<system>Travis CI</system>
		<url>https://travis-ci.org/BigDistributor/BD_Distributor</url>
	</ciManagement>


	<properties>
		<license.licenseName>bsd_2</license.licenseName>
		<license.projectName>BigDistributor</license.projectName>
		<license.organizationName>Preibisch Lab</license.organizationName>
		<license.copyrightOwners>Marwan Zouinkhi, Stephan Preibisch</license.copyrightOwners>

		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
	</properties>

	<repositories>
		<!-- NB: for project parent -->
		<repository>
			<id>imagej.public</id>
			<url>http://maven.imagej.net/content/groups/public</url>
		</repository>

	</repositories>

	<dependencies>
		<dependency>
			<groupId>com.bigdistributor</groupId>
			<artifactId>BD_Distributor</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>

	</dependencies>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<descriptorRefs>
						<descriptorRef>jar-with-dependencies</descriptorRef>
					</descriptorRefs>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>